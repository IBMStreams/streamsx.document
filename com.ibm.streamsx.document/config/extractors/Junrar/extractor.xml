<ExtractorConfiguration>
  <name>Junrar</name>
  <className>com.ibm.streamsx.document.extract.plugins.Junrar.JunrarExtractor</className>
  <execMode>IN_PROCESS</execMode>
  
  <!--  the MB memory limit for single files and in total. Items are usually extracted
  to a buffer, but if the limit is exceeded they are extracted to a temporary disk file.
   <SingleFileMbLimit>2</SingleFileMbLimit>
  <CumulativeMbLimit>100</CumulativeMbLimit>
  -->
 
  <customSettings>
  <entry>
      <string>SingleFileMbLimit</string>
      <string>2</string>
    </entry>
   <entry>
      <string>CumulativeMbLimit</string>
      <string>100</string>
    </entry>
 </customSettings>
 
</ExtractorConfiguration>